global:
  smtp_from: alertmanager@gmail.com
  smtp_smarthost: smtp.gmail.com:587
receivers:
- name: "emailSE"
  email_configs:
{% for receiver in emailSE %}
  - to: {{ receiver.to }}
{% endfor %}
- name: "groupAlert"
  webhook_configs:
{% for receiver in url_webhook_group %}
  - url: {{ receiver.url }}
{% endfor %}
- name: "groupAndCall"
  webhook_configs:
{% for receiver in url_webhook_call %}
  - url: {{ receiver.url }}
{% endfor %}

route:
  receiver: "default"
  group_by: ['alertname']
  group_wait: 30s
  group_interval: 5m
  repeat_interval: 5h
  routes:
  - receiver: emailSE
    match:
      severity: error
  - receiver: groupAlert
    match:
      severity: warning
  - receiver: groupAndCall
    match:
      severity: critical

inhibit_rules:
  - source_match:
      severity: critical
    target_match:
      severity: warning
    equal: ['alertname', 'ip', 'instance']
  - source_match:
      severity: critical
    target_match:
      severity: error
    equal: ['alertname', 'ip', 'instance']  - source_match:
      severity: warning
    target_match:
      severity: error
    equal: ['alertname', 'ip', 'instance']
